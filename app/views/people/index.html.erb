<h1>People#index</h1>
<p>Find me in app/views/people/index.html.erb</p>

<% @people.each do |person| %>
	<%= link_to "#{person.given_name} #{person.family_name}", household_person_path(@household, person) %> 
	<%= link_to 'Destroy', [@household, person], method: :delete, data: { confirm: 'Are you sure?' } %> <br/>
    
<% end %>

<%= link_to 'New Person', new_household_person_path %>
